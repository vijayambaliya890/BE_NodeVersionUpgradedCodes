            if (limitData.limit) {
                isLimit = true;
                isAllowPublish = false;
                //isAllowPublish
                schemeDetails = limitData.details;
                alertMessage = limitData.message;
                if (limitData.status) {
                    isAlert = true;
                } else {
                    isAlert = false;
                }
